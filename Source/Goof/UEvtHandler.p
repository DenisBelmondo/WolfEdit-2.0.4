unit UEvtHandler;interface	uses		ObjIntf, UGoof;implementation	procedure TEvtHandler.IEvtHandler (itsNextHandler: TEvtHandler);	begin		fNextHandler := itsNextHandler;	end;	procedure TEvtHandler.Free;	begin		if gTarget = self then begin				gTarget := nil;				if gFrontWindow <> nil then					gFrontWindow.BecomeTarget				else					gDefaultTarget.BecomeTarget;			end;		inherited Free;	end;	procedure TEvtHandler.BecomeTarget;	begin		if gTarget <> self then begin				if gTarget <> nil then					gTarget.HiliteSelection(false);				gTarget := self;				gTarget.HiliteSelection(true);			end;	end;	procedure TEvtHandler.SetupMenus;	begin		if fNextHandler <> nil then			fNextHandler.SetupMenus;	end;	procedure TEvtHandler.DoMenuCommand (cmdNumber: integer);	begin		if fNextHandler <> nil then			fNextHandler.DoMenuCommand(cmdNumber);	end;	procedure TEvtHandler.Idle;	begin		if fNextHandler <> nil then			fNextHandler.Idle;	end;	procedure TEvtHandler.Key (var e: EventInfo);	begin		if fNextHandler <> nil then			fNextHandler.Key(e);	end;	procedure TEvtHandler.HiliteSelection (entering: boolean);	begin	end;end.