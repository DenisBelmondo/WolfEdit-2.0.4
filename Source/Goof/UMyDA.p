unit UMyDA;{Copy and modify this unit}interface	uses		UGoof;	function OpenDA: OSErr;implementation	type		TMyDA = object(TDA)				function InitialiseMyDA: OSErr;			end;	function OpenDA: OSErr;		var			myDA: TMyDA;	begin		New(myDA);		OpenDA := myDA.InitialiseMyDA;		gDA := myDA;	end;	function TMyDA.InitialiseMyDA: OSErr;		var			result: OSErr;	begin		result := InitialiseDA;		if result = noErr then			;		{your initialisation}		InitialiseMyDA := result;	end;end.